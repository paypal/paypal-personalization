{
  "name": "@paypal/personalization",
  "version": "1.0.0",
  "description": "PayPal Personalization",
  "main": "index.js",
  "scripts": {
    "babel": "babel ./server --ignore=node_modules --out-dir ./server",
    "build": "npm run test && npm run doc",
    "clean": "rimraf dist coverage",
    "debug": "cross-env NODE_ENV=debug",
    "doc": "esdoc",
    "flow": "flow",
    "flow-typed": "rm -rf ./flow-typed && flow-typed install",
    "karma": "cross-env NODE_ENV=test babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/karma start",
    "lint": "eslint src/ test/ *.js",
    "reinstall": "rimraf flow-typed && rimraf node_modules && npm install && flow-typed install",
    "release": "./publish.sh",
    "release:major": "./publish.sh major",
    "release:minor": "./publish.sh minor",
    "release:patch": "./publish.sh patch",
    "setup": "npm install && npm run flow-typed",
    "test": "npm run lint && npm run flow-typed && npm run flow && npm run karma",
    "webpack": "babel-node --plugins=transform-es2015-modules-commonjs ./node_modules/.bin/webpack --progress",
    "prepublishOnly": "npm run babel",
    "postpublish": "rm -rf ./server && git checkout ./server"
  },
  "keywords": [
    "personalization"
  ],
  "author": "Steve Mask",
  "license": "Apache-2.0",
  "dependencies": {
    "@paypal/checkout-components": "^5.0.189",
    "@paypal/sdk-constants": "^1.0.113",
    "beaver-logger": "^4.0.32",
    "belter": "^1.0.146",
    "bowser": "^2.0.0",
    "cross-domain-utils": "^2.0.38",
    "zalgo-promise": "^1.0.48"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "esdoc": "1.1.0",
    "esdoc-flow-type-plugin": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "flow-bin": "0.155.0",
    "grumbler-scripts": "^5.0.0",
    "mocha": "4.1.0",
    "sync-browser-mocks": "2.0.8"
  }
}
